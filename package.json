{
    "name": "vscode-mqsc",
    "displayName": "MQSC",
    "description": "Language support for MQSC commands",
    "icon": "images/icon.png",
    "version": "0.2.1",
    "publisher": "lemrod",
    "repository": "https://github.com/lemrod/vscode-mqsc",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "MQ",
        "mqsc"
    ],
    "contributes": {
        "languages": [
            {
                "id": "mqsc",
                "aliases": [
                    "mqsc"
                ],
                "extensions": [
                    "mqsc"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "mqsc",
                "scopeName": "source.mqsc",
                "path": "./syntaxes/mqsc.tmLanguage.json"
            }
        ]
    }
}
